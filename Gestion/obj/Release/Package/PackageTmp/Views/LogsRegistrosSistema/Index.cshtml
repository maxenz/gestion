@model PagedList.IPagedList<Gestion.Models.LogRegistroSistema>
@using PagedList.Mvc;

@Html.Partial("_Header", Model)

<div class="row">
    <div class="col-lg-8 col-lg-offset-2 ">
        <div class="well well-sm">
            <div class="col-lg-12">
                <form action="@Url.Action("Index")" id="formSearchLogs" method="get" class="searchIndex" data-gestion-ajax="true" data-gestion-target="#logsRegistroSistemaList" >
                    <div class="col-lg-6">
                        <div class="input-group">
                            <input type="text" class="form-control" id="searchName" name="searchName" placeholder="Buscar">
                            <span class="input-group-btn">
                                <button class="btn btn-default btnPrincipal" type="submit" name="search_button" id="search_button"><span class="glyphicon glyphicon-search"></span></button>
                            </span>
                        </div>
                    </div>
                    <div class="col-lg-3">
                        <input type="text" class="form-control datepicker" id="fechaDesde" name="fechaDesde" value="@ViewBag.dftDesde"  data-date-format="dd/mm/yyyy"  >
                    </div>
                    <div class="col-lg-3">
                        <input type="text" class="form-control datepicker" id="fechaHasta" name="fechaHasta" value="@ViewBag.dftHasta"  data-date-format="dd/mm/yyyy" >
                    </div>


                </form>
            </div>
        </div>
    </div>
</div>

@Html.Partial("_Logs", Model)
