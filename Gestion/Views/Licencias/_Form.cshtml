@model Gestion.Models.Licencia

<div class="row">
    <div class="col-md-6 col-md-offset-3">
        <div class="panel panel-primary">
            <div class="panel-heading">
                <h3 class="panel-title">Registro de Terminales</h3>
            </div>
            <div class="panel-body">
                <div class="row">
                    <div class="col-md-6">
                        @Html.TextBoxFor(model => model.Serial, new { @class = "form-control", @placeholder = "Serial de la licencia", autofocus = true })
                        @Html.ValidationMessageFor(model => model.Serial)
                    </div>
                    <div class="col-md-6">
                        @Html.TextBoxFor(model => model.NumeroDeLlave, new { @class = "form-control", @placeholder = "Nro de Hardkey NET"})
                        @Html.ValidationMessageFor(model => model.NumeroDeLlave)
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-4 col-md-offset-4">
                        <input type="submit" value="Guardar" class="btn btn-block btn-primary btnSubmit" />
                    </div>
                </div>
            </div>
        </div>

    </div>
</div>


